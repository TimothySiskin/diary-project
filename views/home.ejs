<%- include('./partials/header.ejs') -%>
<h1>Home</h1>
<p>
  <%= data %>
</p>
<% for(i=0; i < posts.length; i++){ %>
<h1>
  <%= posts[i].title %>
</h1>
<p>
  <%= _.truncate(posts[i].text, {'length' : 100}) %>
  <a href="/posts/<%= _.kebabCase(posts[i].title) %>">Read More</a>
</p>
<% } %> <%- include('./partials/footer.ejs')-%>
